{
  "expo": {
    "name": "Touro",
    "description": "Cross-platform native app for Touro passengers",
    "slug": "touro-passenger",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "scheme": "touropassenger",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/icon_ios.png",
      "supportsTablet": false,
      "bundleIdentifier": "com.scandorama.touropassenger",
      "buildNumber": "1.0.1"
    },
    "android": {
      "icon": "./assets/icon_android.png",
      "package": "com.scandorama.touropassenger",
      "permissions": [
        "READ_PHONE_STATE"
      ],
      "versionCode": 1
    }
  }
}
